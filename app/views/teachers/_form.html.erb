<%= form_for (@teacher) do |f| %>
<div class="field">
<%= f.label :name, "Name:" %>
<%= f.text_field :name %>
<div class="field">
<%= f.label :nationality, "Nationality:" %>
<%= f.text_field :nationality %>
</div><br>
<div class="field">
<%= f.label :skill_ids, "Skills:" %>
<%= f.collection_check_boxes :skill_ids, @skills, :id, :name %>
</div><br>
<div class="actions">
<%= f.submit %>
<% end %>
