<h1> Search For Teachers</h1>

<%= form_tag search_teachers_path, method: :get do %>

<%= text_field_tag :search, nil, placeholder: "Search Nationality" %>
<%= select_tag "skill", options_from_collection_for_select(Skill.all, "id", "name"), placeholder: "skill name Search" %>
<%= submit_tag "search" %>

<% end %>





<%= link_to "Register A New Teacher", new_teacher_path %>
