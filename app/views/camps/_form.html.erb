<%= form_for(@camp) do |f| %>
  <div class="field">
  <%= f.label :username %>
  <%= f.text_field :username %>
  </div><br>
  <div class="field">
  <%= f.label :password %>
  <%= f.password_field :password %>
  </div><br>
  <div class="field">
    <%= f.label :name, "Name of Camp:" %>
    <%= f.text_field :name %>
  </div><br>
  <div class="field">
    <%= f.label :location, "Location:" %>
    <%= f.text_field :location %>
  </div><br>
  <div class="field">
    <%= f.label :cost, "Cost:" %>
    <%= f.number_field :cost %>
  </div><br>
  <div class="field">
    <%= f.label :dates, "Dates:" %>
    <%= f.text_field :dates %>
  </div><br>
  <div class="field">
    <%= f.label :description, "Description:" %>
    <%= f.text_area :description %>
  </div><br>
  <div class="field">
    <%= f.label :photo_url, "Photo:" %>
    <%= f.file_field :photo_url %>
  </div><br>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
